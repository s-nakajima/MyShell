NC3PLUGINS=(); export NC3PLUGINS

if [ "${NC3DIR}" = "" ]; then
	exit 0
fi
cd ${NC3DIR}/app/Plugin

plugins="`ls`"
for plugin in ${plugins}
do
	#skipするプラグイン
	for not in "${SKIP_PLUGINS[@]}"
	do
		if [ "${plugin}" = "${not}" ]; then
			continue 2
		fi
	done
	case "${plugin}" in
		*.bk ) 
			continue
			;;
	esac

	#開発中(業者)プラグイン
	if [ ! "${NORMALDEV}" = "3" ]; then
		for dev0 in "${DEV_PLUGINS[@]}"
		do
			dev=(${dev0})
			if [ "${plugin}" = "${dev}" ]; then
				continue 2
			fi
		done
	fi

	#開発中プラグイン(中島開発)
	if [ "${NORMALDEV}" = "0" -o "${NORMALDEV}" = "2" ]; then
		for dev0 in "${LOCALDEV_PLUGINS[@]}"
		do
			dev=(${dev0})
			if [ "${plugin}" = "${dev}" ]; then
				continue 2
			fi
		done
	fi

	#NC3プラグイン
	NC3PLUGINS=("${NC3PLUGINS[@]}" "${plugin} ${NC3GITURL}")
done

#開発中(業者)プラグイン
